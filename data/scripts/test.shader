textures/test/bricks
{
        qer_editorimage textures/test/bricks.jpg
	q3map_lightmapSize 1024 1024
	q3map_lightmapBrightness 2.0
        {
		tcgen lightmap
                map $lightmap
                rgbgen identity      
        }
        
        {
                map textures/test/bricks.jpg
                blendFunc filter
        }
}

